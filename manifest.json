{

    "name": "TutorialPARTY!!!", 
    "version": "1.0", 
    "manifest_version": 2, 
    "description": "Do you like to party?",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "mypopup.html"
    },
    "icons": {
        "64": "icon.png", 
        "16": "icon.png"
    }, 
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["myscript.js"]

        }
    ],
    
    "content_security_policy": "script_src 'self' 'unsafe-eval' http://www.editpad.org; object-src 'self'"
}







